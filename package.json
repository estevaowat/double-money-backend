{
  "name": "double-money-backend",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Estev√£o Watanabe",
  "license": "MIT",
  "scripts": {
    "start:dev": "tsnd --respawn --transpile-only --ignore-watch node_modules --exit-child --clear src/server.ts",
    "merge-schemas": "npx prisma-merge-schema --datasource ./prisma/schemas/\\* --outputFile ./prisma/schema.prisma",
    "migrate:init": "npx prisma migrate dev",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker compose down",
    "test:int": "yarn docker:up && yarn migrate:init && jest -i --detectOpenHandles --force-exit",
    "prettier-format": "prettier --config .prettierrc.json --write 'src/**/*.ts'",
    "lint": "eslint . --ext .ts --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "@prisma/client": "^2.24.1",
    "@types/node": "^14.14.41",
    "csv-parser": "^3.0.0",
    "express": "^4.17.1",
    "pg": "^8.6.0",
    "ts-jest": "^26.5.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.23",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.22.0",
    "dotenv-cli": "^4.0.0",
    "eslint": "^7.25.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-airbnb": "^0.0.1-security",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "jest": "^26.6.3",
    "jest-mock-extended": "^1.0.15",
    "prettier": "^2.2.1",
    "prisma-merge-schema": "^1.0.0",
    "supertest": "^6.1.3",
    "ts-node-dev": "^1.1.6",
    "tsc": "^1.20150623.0",
    "typescript": "^4.2.4",
    "husky": "^7.0.0"
  }
}
